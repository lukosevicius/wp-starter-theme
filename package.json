{
  "name": "wp-starter-theme",
  "version": "1.0.0",
  "description": "Version 1.2",
  "main": "index.js",
  "scripts": {
    "test": "echo \"hello-world!\"",
    "sass": "node-sass -w -r --source-map true ./assets/sass/main.scss ./assets/css/main.css",
    "sass-prod": "node-sass -r --output-style compressed ./assets/sass/main.scss ./assets/css/main.min.css",
